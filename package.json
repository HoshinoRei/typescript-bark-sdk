{
  "name": "@hoshinorei/bark-sdk",
  "version": "0.6.0",
  "description": "An SDK for Bark written in TypeScript",
  "keywords": [
    "Bark",
    "TypeScript"
  ],
  "homepage": "https://github.com/HoshinoRei/typescript-bark-sdk#readme",
  "bugs": {
    "url": "https://github.com/HoshinoRei/typescript-bark-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HoshinoRei/typescript-bark-sdk.git"
  },
  "license": "MIT",
  "author": "HoshinoRei",
  "type": "module",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "doc": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme",
    "format": "prettier --write .",
    "format:check": "prettier -c .",
    "lint": "eslint --fix . --ignore-path .gitignore",
    "lint:check": "eslint . --ignore-path .gitignore",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "crypto-js": "^4.2.0",
    "is-url": "^1.2.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/is-url": "^1.2.30",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@vitest/coverage-v8": "^2.0.1",
    "axios-mock-adapter": "^2.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sort-class-members": "^1.19.0",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "typedoc": "^0.26.2",
    "typedoc-github-wiki-theme": "^2.0.0",
    "typedoc-plugin-markdown": "^4.0.3",
    "typescript": "^5.1.6",
    "vite": "^5.0.5",
    "vite-plugin-dts": "^4.0.2",
    "vitest": "^2.0.1"
  },
  "engines": {
    "node": "18.x || 20.x || 21.x"
  }
}
